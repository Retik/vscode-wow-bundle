{
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2017,
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "rules": {
        "no-console": "off"
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "globals": {
        "angular": false,
        "acquireVsCodeApi": false
    }
}
